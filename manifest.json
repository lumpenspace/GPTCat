{
  "manifest_version": 3,
    "name": "cladex",
    "version": "1.0",
    "description": "Renders LaTeX formulas for claude.ai",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.claude.ai/*"],
        "allFrames" : true,
        "match_origin_as_fallback": true,
        "match_about_blank": true,
        "run_at" : "document_idle",
        "js": ["content.js"],
        "css": ["lib/katex.min.css"]
      }
    ]
  }
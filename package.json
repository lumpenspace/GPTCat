{
  "name": "gpt-cat",
  "version": "1.0.0",
  "description": "Multi-purpose Claude.ai extensionm adding support for LaTeX and Mermaid diagrams",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "sign": "dotenv -e .env.local web-ext sign --api-key=$WEB_EXT_API_KEY --api-secret=$WEB_EXT_API_SECRET",
    "package": "web-ext build",
    "release": "yarn build && yarn sign && yarn package",
    "build": "parcel build content/content.tsx browserAction/script.ts --dist-dir=./dist",
    "watch": "parcel watch content/content.tsx  --dist-dir=./dist"
  },
  "dependencies": {
    "@types/react": "^18.2.66",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "mermaid": "^10.9.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-webextension": "^2.12.0",
    "@parcel/transformer-sass": "2.12.0",
    "@types/katex": "^0.16.7",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "katex": "^0.16.9",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.0.1",
    "typescript": "*"
  }
}
